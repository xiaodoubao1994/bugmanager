			<a class="quit fl" href="../index.html">
				退出
			</a>
		</div>
	</div>
	<!--导航-->
	<div class="nav_lf fl">
		<ul>
			<li class="skip">项目列表</li>
			<li class="onli skip">bug列表</li>
		</ul>
	</div>
	<!-- 右边内容	 -->
	<div class="right fl">
		<table id="content" border="1" cellpadding="0" cellspacing="0">
			<tbody class="tbody_top">
				<tr>
	                <th>bug编号</th>
