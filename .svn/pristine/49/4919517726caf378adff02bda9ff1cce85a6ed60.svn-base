.head {
	border-bottom:2px solid #0394cb;
	line-height: 80px;
}

.head_left {
	padding-left: 20px;
	line-height:80px;
	color:#0394cb;
	font-size: 30px;
	float: left;
	font-weight: 700;
}

.head_hello {
	color:#0394cb;
}

.nav_lf {
	width: 200px;
	border-right: 2px solid #0394cb;
	height:calc(100% - 82px);
}

.nav_lf ul li {
	width: 100%;
	line-height: 60px;
	color:#333;
	text-align: center;
	font-size: 26px;
	font-weight: 600;
	cursor: pointer;
}

.nav_lf ul li:hover{
	background: #0394cb;
	color:#fff;
}

.nav_lf .onli {
	background: #0394cb;
	color:#fff;
}

.right {
	width:calc(100% - 202px);
	height:calc(100% - 82px);
    background: url(../images/bg.jpg);
    background-size: 100% 100%;
}

.right #content {
	margin:80px auto 0;
	width: 600px;
	background: rgba(255,255,255,.5);
}

.right #content tr {
	line-height: 40px;
	text-align: center;
}

.tbody_top {
	font-size: 16px;
	font-weight: 600;
	color: #000;
}

/*分页*/
.pageBox {
    width: 600px;
    margin: 5px auto;
}

.manageProject_pageBox {
    width: 606px;
    margin: 5px auto;
}

.page {
    float: right;
}

.page .prev {
    display: inline-block;
    height: 30px;
    width: 50px;
    background-color: #0b333f;
    border: 1px solid #3d6705;
    color: #FFFFFF;
    text-align: center;
    line-height: 30px;
    vertical-align: middle;
    cursor: pointer;
}

.page .next {
    display: inline-block;
    height: 30px;
    width: 50px;
    background-color: #0b333f;
    border: 1px solid #3d6705;
    color: #FFFFFF;
    text-align: center;
    line-height: 30px;
    vertical-align: middle;
    cursor: pointer;
}

.page .content {
    list-style: none;
    display: inline-block;
    vertical-align: middle;
}

.page .content li {
    float: left;
    width: 30px;
    height: 30px;
    /*background-color: #fff;*/
    color: #3d6705;
    text-align: center;
    line-height: 30px;
    margin-left: 2px;
    margin-right: 2px;
}

.page .content li:hover {
    background-color: #3d6705;
    color: #fff;
    cursor: pointer;
}

.page .content li.selected {
    background-color: #3d6705;
    color: #fff;
}